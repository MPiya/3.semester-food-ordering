
@{
    ViewData["Title"] = "addProductById";
}

<h1>addProductById</h1>



<div @*style="margin-top: 140px;"*@>

    <table class="table" ">
        <thead>
            @* Define roll of cells*@
            <tr class="text-center">
                @* header cell*@
                <th>Name</th>
                <th>Price</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
            <tr>

                @* data cell*@
                <td class="align-middle">
                    @item.Name
                </td>

                <td class="align-middle">
                @item.Price DKK
                </td>

              
            </tr>
            }
        </tbody>
    </table>
</div>
